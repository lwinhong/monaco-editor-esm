    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.14.1",
