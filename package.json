{
  "name": "monaco-editor-esm",
  "version": "1.8.0",
  "description": "thie editor base on microsoft monaco editor. for toone v3",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@angular/compiler": "^7.2.13",
    "clipboard-polyfill": "^2.8.0",
    "fast-xml-parser": "^3.12.16",
    "htmlparser2": "^3.10.1",
    "iview": "^3.4.0",
